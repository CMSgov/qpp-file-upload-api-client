{
  "name": "qpp-file-upload-api-client",
  "version": "1.3.16",
  "description": "An npm package to send the necessary requests to the Submissions API to execute the functionality required by a file upload client",
  "main": "./dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "coverage": "nyc --reporter=cobertura  --reporter=html mocha --require babel-register --recursive ./test/**/*.spec.js",
    "test": "mocha --require babel-register --recursive ./test/**/*.spec.js",
    "dev": "npm run clean && webpack --progress --colors --watch --env.production=false",
    "build:dev": "npm run clean && webpack --progress --colors --env.production=false",
    "build:prod": "npm run clean && webpack --progress --colors --env.production=true",
    "clean": "rimraf dist",
    "lint": "eslint \"**/*.js\"",
    "pretest": "npm run lint",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test",
    "prepare": "npm run snyk-protect",
    "wizard": "snyk wizard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSgov/qpp-file-upload-api-client.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "cmsgov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CMSgov/qpp-file-upload-api-client/issues"
  },
  "homepage": "https://github.com/CMSgov/qpp-file-upload-api-client#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "snyk": "^1.355.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "eslint": "^7.3.1",
    "lodash": "^4.17.15",
    "mocha": "8.0.1",
    "nyc": "^15.1.0",
    "pre-push": "^0.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^7.2.3",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "pre-push": [
    "test"
  ],
  "snyk": true
}
