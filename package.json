{
  "name": "qpp-file-upload-api-client",
  "version": "1.3.6",
  "description": "An npm package to send the necessary requests to the Submissions API to execute the functionality required by a file upload client",
  "main": "./dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "coverage": "nyc --reporter=html mocha --require babel-register --recursive ./test/**/*.spec.js",
    "test": "snyk test && mocha --require babel-register --recursive ./test/**/*.spec.js",
    "dev": "npm run clean && webpack --progress --colors --watch --env.production=false",
    "build:dev": "npm run clean && webpack --progress --colors --env.production=false",
    "build:prod": "npm run clean && webpack --progress --colors --env.production=true",
    "clean": "rimraf dist",
    "lint": "eslint \"**/*.js\"",
    "pretest": "npm run lint",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "wizard": "snyk wizard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSgov/qpp-file-upload-api-client.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "cmsgov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CMSgov/qpp-file-upload-api-client/issues"
  },
  "homepage": "https://github.com/CMSgov/qpp-file-upload-api-client#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "snyk": "^1.321.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "debug": "^2.6.9",
    "eslint": "^4.19.1",
    "lodash": "^4.17.15",
    "mocha": "5.2.0",
    "nyc": "^15.0.0",
    "pre-push": "^0.1.1",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "pre-push": [
    "test"
  ],
  "snyk": true
}
